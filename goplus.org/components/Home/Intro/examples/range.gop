a := [1, 3, 5, 7, 11]
b := [x*x for x <- a, x > 3]
println b // output: [25 49 121]

mapData := {"Hi": 1, "Hello": 2, "Go+": 3}
reversedMap := {v: k for k, v <- mapData}
