    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-raw": "^6.1.0",
    "trim": ">=0.0.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "@types/react": "^17.0.27",
    "@types/react-copy-to-clipboard": "^5.0.1",
